{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.15",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "vlucas/phpdotenv": "^2.5",
        "oks/yii2-langs": "dev-master",
        "kartik-v/yii2-sortable-input": "v1.2.1",
        "kartik-v/yii2-editable": "@dev",
        "kartik-v/yii2-widget-fileinput": "v1.0.9",
        "2amigos/yii2-ckeditor-widget": "^2.1",
        "yiisoft/yii2-imagine": "~2.1.0",
        "codemix/yii2-localeurls": "dev-master",
        "guzzlehttp/guzzle": "^6.3@dev",
        "mdmsoft/yii2-admin": "dev-master",
        "kartik-v/yii2-tree-manager": "@dev",
        "wbraganca/yii2-fancytree-widget": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "igogo5yo/yii2-upload-from-url": "^1.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\ckeditor\\": "common/modules/2amigos/yii2-ckeditor-widget/src"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
